{
  "extends": ["config:base", "docker:disable"],
  "automerge": true,
  "major": {
    "automerge": false
  },
  "masterIssue": true,
  "baseBranches": ["latest"],
  "packageRules": [
    {
      "baseBranchList": ["latest"],
      "packageNames": ["nexus-plugin-prisma"],
      "enabled": true
    }
  ],
  "ignoreDeps": ["@prisma/cli", "@prisma/client"]
}
